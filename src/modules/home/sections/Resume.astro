---
import GitHubActivity from "../components/resume/GitHubActivity.astro";
import WPM from "../components/resume/WPM.astro";
import Hobbies from "../components/resume/Hobbies.astro";
import Skills from "./Skills.astro";
interface Props{
    lang: string
}
const {lang} = Astro.props;
---
<section class="grid grid-cols-12 gap-5">
    <GitHubActivity lang={lang}/>
    <WPM lang={lang}/>
    <Hobbies lang={lang}/>
    <Skills lang={lang}/>
</section>